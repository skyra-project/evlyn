{
	"name": "evlyn",
	"version": "3.0.0",
	"description": "Monitor and Reviver",
	"main": "dist/Evlyn.js",
	"scripts": {
		"docker:build": "yarn build && docker build -t skyrabot/evlyn .",
		"docker:run": "docker run --rm -d -it --name evlyn skyrabot/evlyn",
		"docker:logs": "docker logs evlyn -f",
		"docker:restart": "docker container rm -f evlyn && yarn docker:run",
		"docker:stop": "docker container stop evlyn",
		"docker:dev": "yarn docker:build && yarn docker:run && yarn docker:logs",
		"start": "node dist/Evlyn.js",
		"lint": "eslint --ext ts src --fix",
		"update": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"dev": "yarn build && yarn start"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/skyra-project/Evlyn.git"
	},
	"keywords": [
		"discord.js",
		"sapphire",
		"reviver",
		"monitor"
	],
	"private": true,
	"author": "Antonio Rom√°n <kyradiscord@gmail.com>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/skyra-project/Evlyn/issues"
	},
	"dependencies": {
		"@klasa/stopwatch": "^0.0.1",
		"@klasa/type": "^0.0.1",
		"@sapphire/decorators": "github:sapphire-project/utilities#build-decorators",
		"@sapphire/framework": "github:sapphire-project/framework#build",
		"@sapphire/utilities": "github:sapphire-project/utilities#build-utilities",
		"colorette": "^1.2.1",
		"discord.js": "^12.3.1",
		"i18next": "^19.7.0",
		"i18next-fs-backend": "^1.0.7",
		"module-alias": "^2.2.2",
		"node-fetch": "^2.6.0",
		"reflect-metadata": "^0.1.13",
		"ws": "^7.3.1",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^9.1.2",
		"@commitlint/config-conventional": "^9.1.2",
		"@sapphire/pieces": "github:sapphire-project/pieces#build",
		"@skyra/eslint-config": "^5.3.1",
		"@types/module-alias": "^2.0.0",
		"@types/node": "^14.6.1",
		"@types/node-fetch": "^2.5.7",
		"@types/ws": "^7.2.6",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^3.10.1",
		"eslint": "^7.7.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.13",
		"prettier": "^2.1.1",
		"pretty-quick": "^3.0.0",
		"typescript": "^4.0.2"
	},
	"_moduleAliases": {
		"@orm": "dist/lib/orm",
		"@utils": "dist/lib/util",
		"@lib": "dist/lib",
		"@root": "dist"
	},
	"resolutions": {
		"acorn": "^7.3.1",
		"chalk": "^4.1.0",
		"fs-nextra": "^0.5.1",
		"lodash": "^4.17.20",
		"minimist": "^1.2.2"
	},
	"engines": {
		"node": ">=14",
		"npm": ">=6"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn pretty-quick --staged && yarn lint-staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
